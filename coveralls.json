{
  "default_stop_words": [
    "defmodule",
    "defrecord",
    "defimpl",
    "def.+(.+\/\/.+).+do"
  ],
  "custom_stop_words": [],
  "coverage_options": {
    "treat_no_relevant_lines_as_covered": true,
    "output_dir": "cover/",
    "minimum_coverage": 96
  },
  "skip_files": [
    "test",
    "deps",
    "lib/just_web.ex",
    "lib/just_web/router.ex",
    "lib/just/application.ex"
  ]
}
